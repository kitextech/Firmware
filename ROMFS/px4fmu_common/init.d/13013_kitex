#!nsh
#
# @name SuperQ Kite
#
# @type Quadrotor x
#
# @output MAIN5 RUDDER
# @output MAIN6 ELEVATOR
#
# @maintainer Andreas Okholm <andreas@kitex.tech>
#

sh /etc/init.d/rc.vtol_defaults

if [ $AUTOCNF == yes ]
then
    param set VT_MOT_COUNT 4
    param set VT_IDLE_PWM_MC  1080
    param set VT_TYPE 1
fi

set MIXER vtol_kitex

set PWM_OUT 1234
set MAV_TYPE 20
